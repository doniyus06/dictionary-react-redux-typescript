{
  "buildOnSave": false,
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./dist/",
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "isolatedModules": false,
    "jsx": "react",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "declaration": false,
    "noImplicitAny": false,
    "noImplicitUseStrict": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceMap": true
  },
  "filesGlob": [
    "./**.d.ts",
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./typings/**/*.ts",
    "./typings/index.d.ts",
    "!./node_modules/**/*"
  ],
  "exclude": [
    "node_modules"
  ],
  "atom": {
    "rewriteTsconfig": false
  }
}
